<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.travel">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SignupActivity"></activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".order.ListOrderActivity" />
        <activity android:name=".order.OrderActivity" />
        <activity android:name=".MoreTripInfoActivity" />
        <activity android:name=".ListAvaliableActivity" />
        <activity android:name=".PickRegionActivity" />
        <activity android:name=".OrderTripActivity" />
        <activity android:name=".SearchAvailableActivity" />
    </application>

</manifest>